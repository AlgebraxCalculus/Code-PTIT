#include <bits/stdc++.h>

using namespace std;

int main() {
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	int a[n];
	for (int i = 0; i < n; i++) {
		cin >> a[i];
	}
	sort(a, a + n);
	cout << max(a[0] * a[1], max(a[n - 1] * a[n - 2], max(a[0] * a[1] * a[n - 1], a[n - 1] * a[n - 2] * a[n - 3])));
	return 0;
}
