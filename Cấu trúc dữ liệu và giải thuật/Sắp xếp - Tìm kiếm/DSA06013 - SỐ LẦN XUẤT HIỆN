#include <bits/stdc++.h>

using namespace std;

int main() {
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--) {
		int n, x;
		cin >> n >> x;
		map<int, int> mp;
		int a[n];
		for (int i = 0; i < n; i++) {
			cin >> a[i];
			mp[a[i]]++;
		}
		if (mp[x] != 0) {
			cout << mp[x] << endl;
		} else {
			cout << -1 << endl;
		}
	}
	return 0;
}
