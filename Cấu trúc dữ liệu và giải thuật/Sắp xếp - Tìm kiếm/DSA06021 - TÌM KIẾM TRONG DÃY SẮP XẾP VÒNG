#include <bits/stdc++.h>

using namespace std;

int main() {
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--) {
		int n, x;
		cin >> n >> x;
		int a[n];
		int ans = 0;
		for (int i = 0; i < n; i++) {
			cin >> a[i];
			if (a[i] == x) {
				ans = i;
			}
		}
		cout << ans + 1 << endl;
	}
	return 0;
}
