#include <bits/stdc++.h>

using namespace std;

int main() {
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--) {
		int n, m;
		cin >> n >> m;
		vector<int> a(n + m);
		for (int i = 0; i < n + m; i++) {
			cin >> a[i];
		}
		sort(a.begin(), a.end());
		for (auto x : a) {
			cout << x << ' ';
		} 
		cout << endl;
	}
	return 0;
}
