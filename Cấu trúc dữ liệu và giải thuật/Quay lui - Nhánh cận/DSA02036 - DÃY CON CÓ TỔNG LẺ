#include <bits/stdc++.h>

using namespace std;

int n, a[1005], x[1005];
vector<vector<int>> v;

void Try (int i, int bd, int sum) {
	for (int j = bd; j <= n; j++) {
		x[i] = a[j];
		if ((sum + a[j]) % 2 == 1) {
			vector<int> tmp(x + 1, x + i + 1);
			v.push_back(tmp);
		}
		Try(i + 1, j + 1, sum + a[j]);
	}
}


bool cmp (vector<int> a, vector<int> b) {
	int i = 0;
	while (i < a.size() && i < b.size()) {
		if (a[i] != b[i]) {
			return a[i] < b[i];
		}
		i++;
	}
	return a.size() < b.size();
}

int main() {
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--) {
		cin >> n;
		memset(x, 0, sizeof(x));
		for (int i = 1; i <= n; i++) {
			cin >> a[i];
		}
		sort(a + 1, a + n + 1, greater<int>());
		Try(1, 1, 0);
		sort(v.begin(), v.end(), cmp);
		for (auto res : v) {
			for (int x : res) {
				cout << x << ' ';
			}
			cout << endl;
		}
		v.clear();
	}
	return 0;
}
