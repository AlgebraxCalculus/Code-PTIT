#include <bits/stdc++.h>

using namespace std;

int main() {
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--) {
		int n, m, k;
		cin >> n >> m >> k;
		vector<int> a(n + m);
		for (int i = 0; i < n + m; i++) {
			cin >> a[i];
		}
		sort(a.begin(), a.end());
		cout << a[k - 1] << endl;
	}
	return 0;
}
