#include <bits/stdc++.h>

using namespace std;

int main() {
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long c, n;
	cin >> c >> n;
	long long w[n];
	for (int i = 0; i < n; i++) {
		cin >> w[i];
	}
	long long dp[25001];
	memset(dp, 0, sizeof(dp));
	for (int i = 0; i < n; i++) {
		for (int j = c; j >= w[i]; j--) {
			dp[j] = max(dp[j], dp[j - w[i]] + w[i]);
		}
	}
	cout << dp[c];
	return 0;
}
