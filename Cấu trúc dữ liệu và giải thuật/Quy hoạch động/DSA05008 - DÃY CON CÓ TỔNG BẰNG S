#include <bits/stdc++.h>

using namespace std;

int main() {
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--) {
		int n, s;
		cin >> n >> s;
		int a[n];
		for (int i = 0; i < n; i++) {
			cin >> a[i];
		}
		bool dp[s + 1];
		for (int i = 0; i <= s; i++) {
			dp[i] = false;
		}
		dp[0] = true;
		for (int i = 0; i < n; i++) {
			for (int j = s; j >= a[i]; j--) {
				if (dp[j - a[i]]) {
					dp[j] = true;
				}
			}
		}
		if (dp[s]) {
			cout << "YES" << endl;
		} else {
			cout << "NO" << endl;
		}
	}
	return 0;
}
