#include <stdio.h>
#include <math.h>
int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        int n;
        scanf("%d", &n);
        int i = sqrt(n);
        if (i * i == n) {
            printf("YES");
        } else {
            printf("NO");
        }
        printf("\n");
    }
    return 0;
}
