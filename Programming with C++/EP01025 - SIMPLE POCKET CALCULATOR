#include <iostream>
#include <iomanip>

using namespace std;

int main() {
	int a, b;
	char x;
	cin >> a >> x >> b;
	if (x == '+') {
		cout << fixed << setprecision(2) << (float) a + b;
	} else if (x == '-') {
		cout << fixed << setprecision(2) << (float) a - b;
	} else if (x == '*') {
		cout << fixed << setprecision(2) << (float) a * b;
	} else {
		cout << fixed << setprecision(2) << (float) a / b;
	}
	return 0;
}
