#include <bits/stdc++.h>

using namespace std;

int snt (int n) {
	if (n < 2) {
		return 0;
	}
	for (int i = 2; i <= sqrt(n); i++) {
		if (n % i == 0) {
			return 0;
		}
	}
	return 1;
}

int main() {
	int t;
	cin >> t;
	while (t--) {
		long long l, r;
		cin >> l >> r;
		int x = sqrt(l), y = sqrt(r), count = 0;
		for (int i = x; i <= y; i++) {
			if (snt(i)) {
				count++;
			}
		}
		cout << count << endl;
	}
	return 0;
}
