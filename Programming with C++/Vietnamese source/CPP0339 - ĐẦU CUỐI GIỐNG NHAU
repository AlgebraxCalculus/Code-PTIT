#include <bits/stdc++.h>

using namespace std;

int main() {
	int t;
	cin >> t;
	while (t--) {
		string s;
		cin >> s;
		map<char, int> mp;
		for (char x : s) {
			mp[x]++;
		}
		long long ans = s.size();
		for (auto x : mp) {
			ans += (x.second * (x.second - 1)) / 2;
		}
		cout << ans << endl;
	}
	return 0;
}
