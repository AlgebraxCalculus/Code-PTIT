#include <bits/stdc++.h>

using namespace std;

int main() {
	int t;
	cin >> t;
	while (t--) {
		int n;
		cin >> n;
		vector<long long> v1, v2;
		long long a[n];
		for (int i = 0; i < n; i++) {
			cin >> a[i];
		}
		for (int i = 0; i < n; i++) {
			if (a[i] != 0) {
				v1.push_back(a[i]);
			} else {
				v2.push_back(a[i]);
			}
		}
		for (auto x : v1) {
			cout << x << ' ';
		}
		for (auto x : v2) {
			cout << x << ' ';
		}
		cout << endl;
	}
	return 0;
}
