#include <bits/stdc++.h>

using namespace std;

int main() {
    int t;
    cin >> t;
    while (t--) {
        int n, m;
        cin >> n >> m;
        int a[n + 1] = {0}, b[m + 1] = {0};
        for (int i = 1; i <= n; i++) {
            cin >> a[i];
            a[i] += a[i - 1];
        }
        for (int i = 1; i<= m; i++) {
            cin >> b[i];
            b[i] += b[i - 1];
        }
        int res = 0;
        for (int i = 1; i <= min(n, m); i++) {
            res = max(res, max(a[i] + b[m] - b[i], b[i] + a[n] - a[i]));
        }
        cout << res << endl;
    }
    return 0;
}
