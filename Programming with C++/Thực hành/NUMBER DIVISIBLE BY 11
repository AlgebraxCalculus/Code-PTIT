#include <bits/stdc++.h>

using namespace std;

int chiadu (string s, int n) {
	int du = 0;
	for (int i = 0; i < s.size(); i++) {
		du = (du * 10 + (s[i] - '0')) % n;
	}
	return du;
}

int main() {
	int t;
	cin >> t;
	while (t--) {
		string s;
		cin >> s;
		if (chiadu(s, 11) == 0) {
			cout << 1 << endl;
		} else {
			cout << 0 << endl;
		}
	}
	return 0;
}
