import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        while (t --> 0) {
            int n = sc.nextInt();
            int h = sc.nextInt();
            for (int i = 1; i < n; i++) {
                double res = Math.sqrt((double) i / n) * h;
                System.out.printf("%.6f ", res);
            }
            System.out.println();
        }
    }
}
