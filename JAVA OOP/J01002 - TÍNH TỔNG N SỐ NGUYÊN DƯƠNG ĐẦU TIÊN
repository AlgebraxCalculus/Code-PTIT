import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);
       int t;
       t = sc.nextInt();
       while (t --> 0) {
           int n;
           n = sc.nextInt();
           System.out.println((long) n * (n + 1) / 2);
       }
    }
}
