#include <bits/stdc++.h>

using namespace std;

int main() {
	int t;
	cin >> t;
	while (t--) {
		int n; 
		cin >> n;
		int cuoi = n % 10;
		int dau = 0;
		while (n > 0) {
			dau = n % 10;
			n /= 10;
		}
		if (dau == cuoi) {
			cout << "YES" << endl;
		} else {
			cout << "NO" << endl;
		}
	}
	return 0;
}
