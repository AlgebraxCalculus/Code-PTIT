#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
    int n;
    scanf("%d", &n);
    int a = n % 10;
    while (n >= 10) {
        n /= 10;
    }
    if (a == n) {
        printf("YES");
    } else {
        printf("NO");
    }
    printf("\n");
    }
    return 0;
}
