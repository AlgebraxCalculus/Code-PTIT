#Liệt kê các số thỏa mãn có ít nhất một số trái dấu đứng cạnh

n = int(input())
a = list(map(int, input().split()))
for i in range(n):
    if i == 0:
        if a[0] * a[1] < 0:
            print(a[0], end = ' ')
    elif i == n - 1:
        if a[-1] * a[-2] < 0:
            print(a[-1], end = ' ')
    else:
        if a[i] * a[i - 1] < 0 or a[i] * a[i + 1] < 0:
            print(a[i], end = ' ')
