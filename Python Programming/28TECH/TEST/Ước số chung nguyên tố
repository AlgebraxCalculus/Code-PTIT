#Kiểm tra tổng chữ số của UCLN có phải là số nguyên tố hay không

from math import *

def nt(n):
    if n < 2:
        return False
    for i in range(2, isqrt(n) + 1):
        if n % i == 0:
            return False
    return True

def check(n):
    s = 0
    while n != 0:
        s += n % 10
        n //= 10
    return nt(s)

for _ in range(int(input())):
    a, b = map(int, input().split())
    x = gcd(a, b)
    if check(x):
        print('YES')
    else:
        print('NO')
