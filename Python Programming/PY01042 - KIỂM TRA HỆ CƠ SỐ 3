def check(s):
    for i in range(0, len(s)):
        if not (s[i] >= '0' and s[i] <= '2'):
            return 0
    return 1

if __name__ == '__main__':
    t = int(input())
    for i in range(t):
        s = input()
        if check(s):
            print('YES')
        else:
            print('NO')
