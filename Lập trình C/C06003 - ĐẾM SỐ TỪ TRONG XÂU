#include <stdio.h>
#include <string.h>

int main() {
    int t; 
    scanf("%d", &t);
    while (t--) {
        scanf("\n"); 
        char xau[205];
        gets(xau);
        int count = 1;
        for (int i = 0; i < strlen(xau) - 1; i++) {
            if (xau[i] == ' ' && xau[i + 1] != ' ') {
                count++;
            }
        }
        printf("%d\n", count);
    }
    return 0;
}
