#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    int a[n + 5][n + 5];
    int fb[105];
    fb[0] = 0, fb[1] = 1, fb[2] = 1;
    for (int i = 3; i < 105; i++) {
        fb[i] = fb[i - 1] + fb[i - 2];
    }
    int hang1 = 0, hang2 = n - 1, cot1 = 0, cot2 = n - 1, giatri = 0;
    while (hang1 <= hang2 && cot1 <= cot2) {
        for (int i = cot1; i <= cot2; i++) {
            a[hang1][i] = fb[giatri++];
        }
        hang1++;
        for (int i = hang1; i <= hang2; i++) {
            a[i][cot2] = fb[giatri++];
        }
        cot2--;
        if (cot1 <= cot2) {
            for (int i = cot2; i >= cot1; i--) {
                a[hang2][i] = fb[giatri++];
            }
            hang2--;
        }
        if (hang1 <= hang2) {
            for (int i = hang2; i >= hang1; i--) {
                a[i][cot1] = fb[giatri++];
            }
            cot1++;
        }
    }
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            printf("%d ", a[i][j]);
        }
        printf("\n");
    }
    return 0;
}
