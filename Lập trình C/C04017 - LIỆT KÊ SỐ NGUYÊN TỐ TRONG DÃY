#include <stdio.h>

int snt (int n) {
	if (n < 2) {
		return 0;
	} else {
		for (int i = 2; i * i <= n; i++) {
			if (n % i == 0) {
				return 0;
			}
		}
	}
	return 1;
}

int main() {
	int n;
	scanf("%d", &n);
	int a[n + 5];
	int count[105] = {0};
	int dem = 0;
	for (int i = 0; i < n; i++) {
		scanf("%d", &a[i]);
		if (snt(a[i])) {
			dem++;
		}
	}
    printf("%d ", dem);
    for (int i = 0; i < n; i++) {
        if (snt(a[i])) {
            printf("%d ", a[i]);
        }
    }
	return 0;
}
