#include <stdio.h>
#include <math.h>
int main() {
	int m, n;
	scanf("%d %d", &m, &n);
	int a = sqrt(m);
	int b = sqrt(n);
	if (a * a == m) {
		printf("%d\n", b - a + 1);
		for (int i = a; i <= b; i++) {
			printf("%d\n", i * i);
		}
	} else if (a * a < m) {
		printf("%d\n", b - a);
		for (int i = a + 1; i <= b; i++) {
			printf("%d\n", i * i);
		}
	}
	return 0;
}

