//Theo thứ tự xuất hiện
#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
	int t;
	scanf("%d", &t);
	while (t--) {
		scanf("\n");
		char s[100];
		gets(s);
		for (int i = 0; i < strlen(s); i++) {
			s[i] = tolower(s[i]);
		}
		char a[50][50];
		char *token = strtok(s, " ");
		int n = 0;
		while (token != NULL) {
			strcpy(a[n++], token);
			token = strtok(NULL, " ");
		}
		int b[100] = {};
		for (int i = 0; i < n; i++) {
			if (b[i] == 0) {
				int count = 1;
				for (int j = i + 1; j < n; j++) {
					if (strcmp(a[i], a[j]) == 0) {
						count++;
						b[j] = 1;
					}
				}
				printf("%s %d\n", a[i], count);
			}
		}
		printf("\n");
	}
	return 0;
}
//Theo thứ tự từ điển
#include <stdio.h>
#include <string.h>
#include <ctype.h>
#include <stdlib.h>

int cmp (const void *a, const void *b) {
	char *x = (char*) a;
	char *y = (char*) b;
	return strcmp(x, y);
}

int main() {
	int t;
	scanf("%d", &t);
	while (t--) {
		scanf("\n");
		char s[100];
		gets(s);
		for (int i = 0; i < strlen(s); i++) {
			s[i] = tolower(s[i]);
		}
		char a[50][50];
		char *token = strtok(s, " ");
		int n = 0;
		while (token != NULL) {
			strcpy(a[n++], token);
			token = strtok(NULL, " ");
		}
		qsort(a, n, sizeof(a[0]), cmp);
		int b[100] = {};
		for (int i = 0; i < n; i++) {
			if (b[i] == 0) {
				int count = 1;
				for (int j = i + 1; j < n; j++) {
					if (strcmp(a[i], a[j]) == 0) {
						count++;
						b[j] = 1;
					}
				}
				printf("%s %d\n", a[i], count);
			}
		}
		printf("\n");
	}
	return 0;
}
