#include <stdio.h>

int main() {
	int t; 
	scanf("%d", &t);
	int lan = 1;
	while (t--) {
		int n;
		scanf("%d", &n);
		int a[n + 5][n + 5];
		int hang1 = 0, hang2 = n - 1, cot1 = 0, cot2 = n - 1, giatri = n * n;
		while (hang1 <= hang2 && cot1 <= cot2) {
		//Hàng đầu tiên
			for (int i = cot1; i <= cot2; i++) {
				a[hang1][i] = giatri--;
			}
			hang1++;
		//Cột cuối cùng
			for (int i = hang1; i <= hang2; i++) {
				a[i][cot2] = giatri--;
			}
			cot2--;
		//Hàng cuối cùng
			if (cot1 <= cot2) {
				for (int i = cot2; i >= cot1; i--) {
					a[hang2][i] = giatri--;
				}
				hang2--;
			}
		//Cột đầu tiên
			if (hang1 <= hang2) {
				for (int i = hang2; i >= hang1; i--) {
					a[i][cot1] = giatri--;
				}
				cot1++;
			}
		}
		printf("Test %d:\n", lan++);
		for (int i = 0; i < n; i++) {
			for (int j = 0; j < n; j++) {
				printf("%d ", a[i][j]);
			}
			printf("\n");
		}
	}
	return 0;
}
