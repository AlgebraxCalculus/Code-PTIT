#include <stdio.h>

int main() {
    int n, m;
    scanf("%d %d", &n, &m);
    int a[105][105] = {};
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            scanf("%d", &a[i][j]);
        }
    }
    int hang1 = 0, hang2 = n - 1, cot1 = 0, cot2 = m - 1, giatri = n * m;
    while (hang1 <= hang2 && cot1 <= cot2) {
        for (int i = cot1; i <= cot2; i++) {
            a[hang1][i] = giatri--;
        }
        hang1++;
        for (int i = hang1; i <= hang2; i++) {
            a[i][cot2] = giatri--;
        }
        cot2--;
        if (cot1 <= cot2) {
            for (int i = cot2; i >= cot1; i--) {
                a[hang2][i] = giatri--;
            }
        }
        hang2--;
        if (hang1 <= hang2) {
            for (int i = hang2; i >= hang1; i--) {
                a[i][cot1] = giatri--;
            }
        }
        cot1++;
    }
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            printf("%d ", a[i][j]);
        }
        printf("\n");
    }
    return 0;
}
